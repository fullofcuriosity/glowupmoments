{
  "framework": null,
  "buildCommand": null,
  "devCommand": "npx serve public -l $PORT",
  "outputDirectory": "public",
  "routes": [
    { "src": "^/api/(.*)$", "dest": "/api/$1" },
    { "src": "^/builder/?$", "dest": "/api/builder" },
    { "src": "^/builder\\.html$", "status": 404 },
    { "src": "^/(.*)$", "dest": "/$1" }
  ]
}
